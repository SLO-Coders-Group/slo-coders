{
  "name": "slo-coders",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:modules": "rm -rf yarn.lock .yarn .pnp.cjs && git checkout .yarn && rm -rf .yarn/cache",
    "audit": "audit-ci --critical",
    "dev": "next dev",
    "dev:validate": "yarn run format:fix && yarn run lint && yarn run type && yarn run test && yarn run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "sdks": "yarn dlx @yarnpkg/sdks",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "format": "yarn run format:fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "type": "tsc --noEmit",
    "validate": "yarn run validate:check",
    "validate:check": "yarn run audit && yarn run format:check && yarn run lint && yarn run type && yarn run test && yarn run build",
    "validate:fix": "yarn run audit && yarn run format:fix && yarn run lint && yarn run type && yarn run test && yarn run build"
  },
  "packageManager": "yarn@3.2.1",
  "dependencies": {
    "@emotion/cache": "~11.9.3",
    "@emotion/react": "~11.9.3",
    "@emotion/server": "~11.4.0",
    "@emotion/styled": "~11.9.3",
    "@fontsource/fira-code": "~4.5.10",
    "@fontsource/oswald": "~4.5.9",
    "@mui/icons-material": "~5.8.4",
    "@mui/material": "~5.9.0",
    "@types/jest": "~28.1.6",
    "clsx": "~1.2.1",
    "jest-fetch-mock": "~3.0.3",
    "next": "~12.2.2",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~13.3.0",
    "@types/node": "~18.0.5",
    "@types/prettier": "~2.6.3",
    "@types/react": "~18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/testing-library__jest-dom": "~5.14.5",
    "@typescript-eslint/eslint-plugin": "~5.30.6",
    "@typescript-eslint/parser": "~5.30.6",
    "audit-ci": "~6.3.0",
    "eslint": "~8.20.0",
    "eslint-config-next": "~12.2.2",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-jest": "~26.6.0",
    "eslint-plugin-testing-library": "~5.5.1",
    "eslint-plugin-unicorn": "~43.0.1",
    "jest": "~28.1.3",
    "jest-environment-jsdom": "~28.1.3",
    "prettier": "~2.7.1",
    "typescript": "~4.7.4"
  }
}
