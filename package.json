{
  "name": "slo-coders",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:modules": "rm -rf yarn.lock .yarn .pnp.cjs && git checkout .yarn && rm -rf .yarn/cache",
    "audit": "audit-ci --critical",
    "dev": "next dev",
    "dev:validate": "yarn run format:fix && yarn run lint && yarn run type && yarn run test && yarn run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "sdks": "yarn dlx @yarnpkg/sdks",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "format": "yarn run format:fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "type": "tsc --noEmit",
    "validate": "yarn run validate:check",
    "validate:check": "yarn run audit && yarn run format:check && yarn run lint && yarn run type && yarn run test && yarn run build",
    "validate:fix": "yarn run audit && yarn run format:fix && yarn run lint && yarn run type && yarn run test && yarn run build"
  },
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "@emotion/cache": "~11.11.0",
    "@emotion/react": "~11.11.1",
    "@emotion/server": "~11.11.0",
    "@emotion/styled": "~11.11.0",
    "@fontsource/fira-code": "~5.0.15",
    "@fontsource/oswald": "~5.0.17",
    "@mui/icons-material": "~5.14.18",
    "@mui/material": "~5.14.18",
    "@types/jest": "~29.5.9",
    "clsx": "~2.0.0",
    "jest-fetch-mock": "~3.0.3",
    "luxon": "~3.4.4",
    "next": "~14.0.3",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~6.1.4",
    "@testing-library/react": "~14.1.2",
    "@types/luxon": "~3.3.5",
    "@types/node": "~20.9.3",
    "@types/prettier": "~3.0.0",
    "@types/react": "~18.2.38",
    "@types/react-dom": "18.2.16",
    "@typescript-eslint/eslint-plugin": "~6.12.0",
    "@typescript-eslint/parser": "~6.12.0",
    "audit-ci": "~6.6.1",
    "eslint": "~8.54.0",
    "eslint-config-next": "~14.0.3",
    "eslint-config-prettier": "~9.0.0",
    "eslint-plugin-jest": "~27.6.0",
    "eslint-plugin-testing-library": "~6.1.2",
    "eslint-plugin-unicorn": "~49.0.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "~29.7.0",
    "prettier": "~3.1.0",
    "typescript": "~5.3.2"
  }
}
