{
  "name": "slo-coders",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean:modules": "rm -rf yarn.lock .yarn .pnp.cjs && git checkout .yarn && rm -rf .yarn/cache",
    "audit": "audit-ci --critical",
    "dev": "next dev",
    "dev:validate": "yarn run format:fix && yarn run lint && yarn run type && yarn run test && yarn run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "sdks": "yarn dlx @yarnpkg/sdks",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "format": "yarn run format:fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "type": "tsc --noEmit",
    "validate": "yarn run validate:check",
    "validate:check": "yarn run audit && yarn run format:check && yarn run lint && yarn run type && yarn run test && yarn run build",
    "validate:fix": "yarn run audit && yarn run format:fix && yarn run lint && yarn run type && yarn run test && yarn run build"
  },
  "packageManager": "yarn@3.2.3",
  "dependencies": {
    "@emotion/cache": "~11.10.3",
    "@emotion/react": "~11.10.4",
    "@emotion/server": "~11.10.0",
    "@emotion/styled": "~11.10.4",
    "@fontsource/fira-code": "~4.5.11",
    "@fontsource/oswald": "~4.5.10",
    "@mui/icons-material": "~5.10.3",
    "@mui/material": "~5.10.4",
    "@types/jest": "~29.0.0",
    "clsx": "~1.2.1",
    "jest-fetch-mock": "~3.0.3",
    "luxon": "~3.4.2",
    "next": "~12.2.5",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "~5.16.5",
    "@testing-library/react": "~13.4.0",
    "@types/luxon": "~3.3.2",
    "@types/node": "~18.7.16",
    "@types/prettier": "~2.7.0",
    "@types/react": "~18.0.18",
    "@types/react-dom": "18.0.6",
    "@types/testing-library__jest-dom": "~5.14.5",
    "@typescript-eslint/eslint-plugin": "~5.36.2",
    "@typescript-eslint/parser": "~5.36.2",
    "audit-ci": "~6.3.0",
    "eslint": "~8.23.0",
    "eslint-config-next": "~12.2.5",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-jest": "~27.0.1",
    "eslint-plugin-testing-library": "~5.6.2",
    "eslint-plugin-unicorn": "~43.0.2",
    "jest": "~29.0.2",
    "jest-environment-jsdom": "~29.0.2",
    "prettier": "~2.7.1",
    "typescript": "~4.8.2"
  }
}
