{
  "name": "slo-coders",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean:modules": "rm -rf yarn.lock .yarn .pnp.cjs && git checkout .yarn && rm -rf .yarn/cache",
    "audit": "audit-ci --critical",
    "dev": "next dev",
    "dev:validate": "yarn run format:fix && yarn run lint && yarn run type && yarn run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "sdks": "yarn dlx @yarnpkg/sdks",
    "format": "yarn run format:fix",
    "format:fix": "prettier --write .",
    "format:check": "prettier --check .",
    "type": "tsc --noEmit",
    "validate": "yarn run audit && yarn run format:fix && yarn run lint && yarn run type && yarn run build"
  },
  "packageManager": "yarn@4.0.2",
  "dependencies": {
    "@emotion/cache": "~11.11.0",
    "@emotion/react": "~11.11.1",
    "@emotion/server": "~11.11.0",
    "@emotion/styled": "~11.11.0",
    "@mui/icons-material": "~5.14.19",
    "@mui/material": "~5.14.20",
    "@next/third-parties": "14.0.4",
    "clsx": "~2.0.0",
    "luxon": "~3.4.4",
    "next": "~14.0.4",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "~14.1.2",
    "@types/luxon": "~3.3.7",
    "@types/node": "~20.10.4",
    "@types/prettier": "~3.0.0",
    "@types/react": "~18.2.43",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "~6.13.2",
    "@typescript-eslint/parser": "~6.13.2",
    "audit-ci": "~6.6.1",
    "eslint": "~8.55.0",
    "eslint-config-next": "~14.0.4",
    "eslint-config-prettier": "~9.1.0",
    "eslint-plugin-testing-library": "~6.2.0",
    "eslint-plugin-unicorn": "~49.0.0",
    "prettier": "~3.1.0",
    "typescript": "~5.3.3"
  }
}
